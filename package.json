{
  "name": "muban-storybook",
  "version": "1.0.0",
  "description": "A storybook for Muban projects, preview components and their source.",
  "bin": {
    "build-storybook": "./bin/build.js",
    "start-storybook": "./bin/index.js",
    "storybook-server": "./bin/index.js"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --config build-tools/config/webpack/webpack.conf.dev.js",
    "build": "cross-env NODE_ENV=production node build-tools/script/build.js",
    "lint:js": "eslint src --ext .js,.vue --cache",
    "analyze": "npm run build -- --analyze",
    "lint:ts": "tslint src/**/*.ts -p tsconfig.json",
    "lint:scss": "stylelint \"./src/**/*.scss\" --cache",
    "preview": "node build-tools/script/preview-server.js",
    "prettify": "prettier --write \"src/**/*.{js,ts,scss}\"",
    "precommit": "lint-staged",
    "postcommit": "git reset",
    "build:babel": "babel ./src -x \".ts\" -x \".js\" --out-dir ./dist --copy-files",
    "server": "node src/server/start.js -p 8081 -c src/demo/storybook",
    "server:build": "node src/server/start-build.js -c src/demo/storybook -o ./dist/storybook"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{js,ts,scss,json}": [
        "prettier --write",
        "git add"
      ],
      "src/**/*.js": [
        "eslint --ext .js --cache"
      ],
      "src/**/*.ts": [
        "tslint -c .tslintrc.js -p tsconfig.json -t verbose"
      ],
      "src/**/*.scss": [
        "stylelint --cache"
      ]
    }
  },
  "author": "Arjan van Wijk <arjan@mediamonks.com> (ThaNarie)",
  "license": "MIT",
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "last 3 IOS versions",
    "not ie <= 9",
    "not ExplorerMobile <= 11"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime-corejs2": "^7.1.5",
    "@types/node": "^10.12.11",
    "axios": "^0.18.0",
